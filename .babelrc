{
    "presets": ["env"],
    "plugins": [
        ["transform-class-properties", { "spec": true }],
        "transform-decorators-legacy",
        ["transform-runtime", {
            "helpers": false, 
            "polyfill": false, 
            "regenerator": true, 
            "moduleName": "babel-runtime"
          }
        ]
    ]
}